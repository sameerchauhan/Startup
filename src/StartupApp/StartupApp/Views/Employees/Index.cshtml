@model IEnumerable<Web.UI.Models.EmployeeListModel>

@{
    ViewBag.Title = "title";
}

<div class="row">
    <div class="col-md-12 jumbotron">
        <h2>Employees</h2>
    </div>
    
    <table>
        <thead><th>Name</th></thead>
        @foreach (var employee in Model)
        {
            <tr><td>@employee.PersonFirstName</td></tr>
        }
    </table>
    
</div>